{
  "name": "StudyPlanner Backend",
  "dockerComposeFile": [
    "../../docker-compose.yml"
  ],
  "service": "server",
  "workspaceFolder": "/server",
  "shutdownAction": "none",
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python",
    "python.linting.enabled": true
  }
}